    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-github": "^4.3.5",
    "husky": "^7.0.4",
    "rimraf": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./.github/husky/pre-commit",
      "pre-push": "./.github/husky/pre-push"
