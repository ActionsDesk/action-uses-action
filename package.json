  "license": "MIT",
  "repository": "github:ActionsDesk/report-action-usage",
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "main": "action.js",
  "scripts": {
    "build": "npx @vercel/ncc build action.js --out dist --license licenses.txt --quiet --minify",
